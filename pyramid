#include <stdio.h>

int main()
{
    int i,j,height;
    printf("\n Enter the height:");
    scanf("%d",&height);
        for(i=1;i<=height;i++)
        {
            for(j=1;j<=i;j++)
            {
                printf("%d",j);
            }
            printf ("\n");
        }
    

    return 0;
}
